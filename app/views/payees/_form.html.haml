= simple_form_for(@payee) do |f|
  = f.error_notification

  .form-inputs
    = f.input :nickname
    = f.input :account_holder
    = f.input :account_number
    = f.input :bank_name
    = f.input :branch_name

  .form-actions
    .well.col-sm-9.col-sm-offset-3
      = f.button :submit, :class=> 'btn-primary'

.clearfix
